    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "2.3.0",
    "css-loader": "^3.6.0",
    "dotenv": "4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "3.2.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^5.1.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-perf": "^2.0.8",
    "file-loader": "^5.0.2",
    "fs-extra": "^9.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^0.9.0",
    "object-assign": "4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.13",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "^2.0.9",
    "promise": "8.0.1",
    "purify-css": "^1.2.6",
    "purifycss-webpack": "^0.7.0",
    "react-dev-utils": "^11.0.4",
    "react-error-overlay": "^3.0.0",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "0.6.2",
    "webpack": "^4.41.5",
    "webpack-cleanup-plugin": "0.5.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^2.2.0",
    "whatwg-fetch": "2.0.3",
    "why-did-you-update": "^0.1.1"
  },
  "optionalDependencies": {
    "fsevents": "^1.2.9"
  },
  "dependencies": {
    "draft-js": "^0.11.7",
    "draftjs-md-converter": "^1.5.1",
    "gtex-d3": "github:broadinstitute/gtex-viz#8d65862",
    "igv": "^2.8.6",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "js-cookie": "^2.2.1",
    "jsoneditor": "^9.5.6",
    "jsoneditor-react": "^3.1.1",
    "lodash": "^4.17.21",
    "object-hash": "^1.3.0",
    "pedigreejs": "github:CCGE-BOADICEA/pedigreejs#2e17296",
    "prop-types": "^15.7.2",
    "query-string": "^6.1.0",
    "random-material-color": "1.0.3",
    "react": "^16.14.0",
    "react-document-title": "2.0.3",
    "react-dom": "^16.14.0",
    "react-error-boundary": "^3.1.3",
    "react-markdown": "^5.0.3",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^5.1.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "react-xhr-uploader": "^0.4.4",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "styled-components": "^5.3.0",
    "timeago.js": "3.0.2",
    "timeout-as-promise": "1.0.0",
    "typescript": "^2.9.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/**/?*.(test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "node_modules/.+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "igv": "<rootDir>/config/jest/fileTransform.js",
      "jsoneditor-react": "<rootDir>/config/jest/fileTransform.js",
      "gtex-d3": "<rootDir>/config/jest/fileTransform.js",
      "pedigreejs": "<rootDir>/config/jest/fileTransform.js",
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ],
        "presets": [
          "@babel/env",
          "@babel/preset-react"
        ]
      }
    },
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties",
      [
        "babel-plugin-styled-components",
        {
          "displayName": true
        }
      ],
      [
        "babel-plugin-module-resolver",
        {
          "root": [
            "./"
          ],
          "extensions": [
            ".js",
            ".jsx",
            ".css"
          ],
          "alias": {
            "shared": "./shared/",
            "pages": "./pages/",
            "gtex-d3": "./node_modules/gtex-d3/"
          }
        }
      ]
    ],
    "presets": [
      "@babel/preset-react",
      [
        "@babel/env",
        {
          "modules": false
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/populationgenomics/seqr.git"
  },
  "license": "AGPL-3.0"
}
